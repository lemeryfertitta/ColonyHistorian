{
  "name": "Colony Historian",
  "description": "Recorder for Colonist.io games",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "media/icons/icon_16.png",
    "48": "media/icons/icon_48.png",
    "128": "media/icons/icon_128.png"
  },
  "content_scripts": [
    {
      "world": "MAIN",
      "js": ["src/recorder/recorder.js"],
      "matches": ["https://colonist.io/*"],
      "run_at": "document_start"
    }
  ],
  "permissions": []
}
